{
  "name": "ledmatrix",
  "version": "0.0.1",
  "description": "node-webkit and (restricted) web Application for controlling a led matrix and also playing games.",
  "main": "index.html",
  "scripts": {
    "postinstall": "./postinstall.sh",
    "start": "nodewebkit",
    "build-nodewebkit": "NODE_ENV=nodewebkit webpack -d --progress --watch --colors",
    "build-web": "NODE_ENV=web webpack -p --progress",
    "test": "NODE_ENV=test jest"
  },
  "window": {
    "toolbar": false,
    "frame": false,
    "width": 860,
    "height": 640
  },
  "author": "Ciro S. Costa <ciro9758@gmail.com> (http://cirocosta.github.io/)",
  "license": "MIT",
  "jest": {
    "scriptPreprocessor": "./jsx-transform.js",
    "unmockedModulePathPatterns": [
      "./node_modules/react"
    ],
    "testFileExtensions": [
      "js",
      "jsx"
    ]
  },
  "devDependencies": {
    "css-loader": "^0.9.0",
    "express": "^4.10.2",
    "jest-cli": "^0.1.18",
    "jsx-loader": "^0.12.1",
    "keymaster": "^1.6.2",
    "matrix-snake": "0.0.2",
    "mdns": "^2.2.1",
    "node-pre-gyp": "^0.5.27",
    "nodewebkit": "^0.8.6",
    "nw-gyp": "^0.12.2",
    "object-assign": "^1.0.0",
    "react": "^0.12.0",
    "react-matrix": "0.0.6",
    "react-tools": "^0.12.0",
    "sass-loader": "^0.2.0",
    "socket.io": "^1.2.0",
    "style-loader": "^0.8.0",
    "webpack": "^1.4.3",
    "yaspm": "0.0.2"
  }
}
